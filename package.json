{
  "name": "simple-bundling-js-backend-frontend-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "trash dist",
    "copy-assets": "cpy frontend/assets/**/* dist/assets",
    "dev": "run-s clean copy-assets dev-watch",
    "dev-watch": "concurrently npm:dev-watch-*",
    "dev-watch-parcel": "delay 2 && cross-env NODE_ENV=development parcel watch frontend/js/index.js --out-dir dist/bundles",
    "dev-watch-server": "cross-env NODE_ENV=development nodemon --watch backend --ext js,liquid backend/index.js",
    "prod":"run-s lint jest prod-parcel-build",
    "prod-parcel-build": "cross-env NODE_ENV=production parcel build frontend/js/index.js --out-dir dist/bundles --no-source-maps",
    "test": "jest",
    "lint": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "concurrently": "^5.1.0",
    "cpy-cli": "^3.0.0",
    "delay-cli": "^1.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "trash-cli": "^3.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "internal-ip": "^5.0.0",
    "liquidjs": "^9.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkle/simple-bundling-js-backend-frontend-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Darkle/simple-bundling-js-backend-frontend-boilerplate/issues"
  },
  "homepage": "https://github.com/Darkle/simple-bundling-js-backend-frontend-boilerplate#readme"
}
